  },
  "homepage": "https://github.com/EmptyOps/woocommerce-bundle-choice#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bufferutil": "^4.0.1",
    "chai": "^4.2.0",
